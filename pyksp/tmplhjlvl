
import rope.base.project
from rope.base import libutils

myproject = rope.base.project.Project(
    'E:/packages/pyksp')
#myproject.validate('E:/packages/pyksp/pyksp/base_types.py')
myproject.validate()
ropy = libutils.path_to_resource(myproject, 'E:/packages/pyksp/pyksp/base_types.py')

from rope.refactor.extract import ExtractMethod
changes = ExtractMethod(myproject, ropy, 9589, 9877).get_changes('_make_type_prefix', similar=True)
myproject.do(changes)

myproject.close()
